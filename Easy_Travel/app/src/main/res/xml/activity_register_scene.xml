<?xml version="1.0" encoding="utf-8"?>
<MotionScene 
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:motion="http://schemas.android.com/apk/res-auto">

    <Transition
        motion:constraintSetEnd="@+id/end"
        motion:constraintSetStart="@id/start"
        motion:duration="1000">
       <KeyFrameSet>
       </KeyFrameSet>
    </Transition>

    <ConstraintSet android:id="@+id/start">
        <Constraint
            android:layout_height="60dp"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_width="258dp"
            android:id="@+id/email"
            motion:layout_constraintTop_toBottomOf="@+id/textView3"
            android:layout_marginTop="88dp" />
        <Constraint
            android:layout_height="60dp"
            android:layout_width="258dp"
            android:id="@+id/password2"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintHorizontal_bias="0.496"
            motion:layout_constraintTop_toBottomOf="@+id/password"
            android:layout_marginTop="28dp" />
        <Constraint
            android:layout_height="60dp"
            android:layout_width="258dp"
            android:id="@+id/password"
            motion:layout_constraintStart_toStartOf="parent"
            motion:layout_constraintEnd_toEndOf="parent"
            motion:layout_constraintHorizontal_bias="0.497"
            motion:layout_constraintTop_toBottomOf="@+id/email"
            android:layout_marginTop="20dp" />
        <Constraint
            android:layout_height="50dp"
            android:layout_width="201dp"
            android:id="@+id/registerButton"
            motion:layout_constraintTop_toBottomOf="@+id/password2"
            android:layout_marginTop="124dp"
            motion:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="60dp"
            android:layout_marginRight="60dp" />
    </ConstraintSet>

    <ConstraintSet android:id="@+id/end">
    </ConstraintSet>
</MotionScene>